<h1>Listing Plans</h1>

<table>
  <tr>
    <th>Plan Name</th>
    <th></th>
    <th></th>
  </tr>

  <% @plans.each do |plan| %>
    <tr>
      <td><%= plan.name %></td>
      <td> <%= link_to 'Show', plan_path(plan) %></td>
      <td> <%= link_to 'Edit', edit_plan_path(plan) %></td>
    </tr>
  <% end %>
</table>

<a href="/plans/new">Create New Plan</a>
